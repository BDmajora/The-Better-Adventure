{
  "multipart": [
    {
      "apply": [
        {
          "model": "backport:block/bamboo/bamboo1_age0"
        },
        {
          "model": "backport:block/bamboo/bamboo2_age0"
        },
        {
          "model": "backport:block/bamboo/bamboo3_age0"
        },
        {
          "model": "backport:block/bamboo/bamboo4_age0"
        }
      ],
      "when": {
        "age": "0"
      }
    },
    {
      "apply": [
        {
          "model": "backport:block/bamboo/bamboo1_age1"
        },
        {
          "model": "backport:block/bamboo/bamboo2_age1"
        },
        {
          "model": "backport:block/bamboo/bamboo3_age1"
        },
        {
          "model": "backport:block/bamboo/bamboo4_age1"
        }
      ],
      "when": {
        "age": "1"
      }
    },
    {
      "apply": {
        "model": "backport:block/bamboo/bamboo_small_leaves"
      },
      "when": {
        "leaves": "small"
      }
    },
    {
      "apply": {
        "model": "backport:block/bamboo/bamboo_large_leaves"
      },
      "when": {
        "leaves": "large"
      }
    }
  ]
}
